<!doctype html>
<html>
<head>
    <% include ../Components/settings.html %>
    <title>BUFF-我的自选股</title>
</head>
<body>
<div class="page-container">

    <% include ../Components/navigator.ejs %>

    <div class="panel panel-default">
        <div class="panel-heading">我的自选股</div>
        <div class="panel-body"></div>

        <div id="my_stocks" class="col-md-12 tab-pane">
            <div>
                <table class="table table-hover">
                    <thead>
                    <tr>
                        <th>股票名称</th>
                        <th>股票代码</th>
                        <th>现价</th>
                        <th>涨跌额</th>
                        <th>涨跌幅</th>
                        <th>昨收</th>
                        <th>今开</th>
                        <th>最高</th>
                        <th>最低</th>
                        <th>成交量</th>
                        <th>成交额</th>
                        <th>总市值</th>
                        <th>流通市值</th>
                        <th>换手率</th>
                        <th>市净率</th>
                        <th>振幅</th>
                        <th>市盈率</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(let i = 0; i < stockNames.length; i++) { %>
                    <tr>
                        <td><%= stockNames[i][0] %></td>
                        <td><%= stockNames[i][1] %></td>
                        <td><%= stockInfos[i][0] %></td>
                        <td><%= stockInfos[i][1] %></td>
                        <td><%= stockInfos[i][2] %></td>
                        <td><%= stockInfos[i][3] %></td>
                        <td><%= stockInfos[i][4] %></td>
                        <td><%= stockInfos[i][5] %></td>
                        <td><%= stockInfos[i][6] %></td>
                        <td><%= stockInfos[i][7] %></td>
                        <td><%= stockInfos[i][8] %></td>
                        <td><%= stockInfos[i][9] %></td>
                        <td><%= stockInfos[i][10] %></td>
                        <td><%= stockInfos[i][11] %></td>
                        <td><%= stockInfos[i][12] %></td>
                        <td><%= stockInfos[i][13] %></td>
                        <td><%= stockInfos[i][14] %></td>
                    </tr>
                    <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <a class="bloc-button btn btn-d scrollToTop" onclick="scrollToTarget('1')"><span
                class="fa fa-chevron-up"></span></a>

</div>
</body>

<% include ../Components/footer.ejs %>
</html>
